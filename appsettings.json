{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*",
  "CorsSettings": {
    "AllowedOrigins": ["*"],
    "AllowedMethods": ["GET", "POST", "PUT", "DELETE", "OPTIONS"],
    "AllowedHeaders": ["*"],
    "AllowCredentials": false,
    "PreflightMaxAge": 3600
  },
  "ExternalApi": {
    "BaseUrl": "https://dummyjson.com",
    "TimeoutSeconds": 30,
    "RetryCount": 3
  }
}